<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record id="view_user_form_inherited" model="ir.ui.view">
			<field name="name">res.user.form.in.branch</field>
			<field name="model">res.users</field>
			<field name="inherit_id" ref="base.view_users_form"/>
			<field name="arch" type="xml">
				<xpath expr="//sheet/notebook/page[2]/group[1]" position="before">
					<group name="branch">
						<field name="branch_ids" widget="many2many_tags" context="{'see_all_branch':1}"/>
						<field name="branch_id"/>
					</group>
				</xpath>
			</field>
		</record>
	</data>
</odoo>